{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/js/(.*)",
      "dest": "/src/public/js/$1"
    },
    {
      "src": "/(.*).html",
      "dest": "/src/public/$1.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/src/public/$1"
    }
  ]
}
