{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "handle": "filesystem"
      },
      {
        "src": "/(.*)",
        "dest": "/src/public/$1",
        "status": 200
      },
      {
        "src": "/(.*)",
        "dest": "/index.js"
      }
    ]
  }
  